#include<iostream>
using namespace std;
int main()
{
	int a,b,c;
	char op;
	cout<<"enter the a value";
	cin>>a;
	cout<<"enter the b value";
	cin>>b;
	cout<<"enter the operation";
	cin>>op;
	switch(op)
	{
		case'+':
			c=a+b;
			cout<<c;
			break;
			case'-':
				c=a-b;
				cout<<c;
				break;
				case'*':
					c=a*b;
					cout<<c;
					break;
					case'/':
						c=a/b;
						cout<<c;
						break;
						case'%':
							c=a%b;
							cout<<c;
							break;
							}
	return 0;
}
