#include <iostream>
#include <string>
using namespace std;
int countElements(const string& str) {
    return str.length();
}

int main() {
    string inputStr;
    cout << "Enter a string: ";
    getline(std::cin, inputStr);

    int numElements = countElements(inputStr);

    cout << "Number of elements in the string: " << numElements <<endl;

    return 0;
}
