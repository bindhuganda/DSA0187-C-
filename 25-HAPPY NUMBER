#include<iostream>
using namespace std;
int main()
{
	int n,r,sum=0,temp;
	cout<<"enter the number";
	cin>>n;
	while(sum!=1 && sum!=4)
	{
	sum=0;
	while(n>0)
	{
		r=n%10;
		sum=sum+(r*r);
		n=n/10;
	}
	n=sum;
	}
	if(sum==1)
	{
		cout<<"happy number";
	}
	else
	{
		cout<<"not happy number";
	}
	return 0;
}
